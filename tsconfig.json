{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["DOM", "ESNext"],
    "allowJs": true,
    "checkJs": true,
    "jsx": "preserve",
    "moduleResolution": "Node",
    "isolatedModules": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "astro:content": ["./.astro/types.d.ts"]
    }
  },
  "include": ["src", ".astro"]
}